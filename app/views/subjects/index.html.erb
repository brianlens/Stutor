<h1>Subjects#index</h1>
<p>Find me in app/views/subjects/index.html.erb</p>

<h1>Subjects</h1>

<table>
  <thead>
    <tr>
      <th>Subject</th>
      <th>Description</th>
      <th>Price</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @subjects.each do |subject| %>
      <tr>
        <td><%= subject.subject %></td>
    <% end %>
  </tbody>
</table>
